[color]
  ui = true
[core]
  excludesfile = /home/anhtran/.gitignore_global
  editor       = vim
[user]
  name  = anhtrantuan
  email = anhtrantuan.hcmc@gmail.com
[push]
  default = simple
[web]
  browser = google-chrome
[alias]
  aa = add -A
  ap = add -p
  co = checkout
